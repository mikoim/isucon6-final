{% package main %}

{% func SVG(r *Room) %}<?xml version="1.0" standalone="no"?><!DOCTYPE svg PUBLIC "-//W3C//DTD SVG 1.1//EN" "http://www.w3.org/Graphics/SVG/1.1/DTD/svg11.dtd"><svg xmlns="http://www.w3.org/2000/svg" version="1.1" baseProfile="full" width="{%d r.CanvasWidth %}" height="{%d r.CanvasHeight %}" style="width:{%d r.CanvasWidth %}px;height:{%d r.CanvasHeight %}px;background-color:white;" viewBox="0 0 {%d r.CanvasWidth %} {%d r.CanvasHeight %}">{% for _, s := range r.Strokes %}<polyline id="{%v s.ID %}" stroke="rgba({%d s.Red %},{%d s.Green %},{%d s.Blue %},{%f s.Alpha %})" stroke-width="{%d s.Width %}" stroke-linecap="round" stroke-linejoin="round" fill="none" points="{% for _, p := range s.Points %}{%f p.X %},{%f p.Y %} {% endfor %}"></polyline>{% endfor %}</svg>{% endfunc %}
